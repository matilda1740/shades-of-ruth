mat\n                        value={price}\n                        displayType={'text'}\n                        thousandSeparator={true}\n                        prefix={'Ksh. '} />\n                </h4>\n                <p>{description}</p>\n            </div>\n    )\n}\n","/Users/matildamwendwa/Desktop/shades/src/Components/AllProducts.js",[],"/Users/matildamwendwa/Desktop/shades/src/Components/StateProvider.js",[],"/Users/matildamwendwa/Desktop/shades/src/Components/reducer.js",[],"/Users/matildamwendwa/Desktop/shades/src/Components/WishList.js",[],"/Users/matildamwendwa/Desktop/shades/src/Components/CheckOut.js",[],"/Users/matildamwendwa/Desktop/shades/src/Components/ListProduct.js",["90","91"],"import React from 'react'\nimport './ListProduct.css'\nimport DeleteRoundedIcon from '@material-ui/icons/DeleteRounded';\nimport { ShoppingBasketRounded } from '@material-ui/icons'\n\nimport { useStateValue } from './StateProvider';\nimport CurrencyFormat from 'react-currency-format';\n\nexport default function ListProduct({id, type, name, image, description, price}) {\n\n        const [{cart, wishlist}, dispatch ] = useStateValue(); \n\n        const removeFromWishList = () => {\n            dispatch({\n                type: \"remove_from_wishlist\",\n                id\n            });\n        };\n        \n        const addToCart = () => {\n        dispatch({\n            type: \"add_to_cart\", \n            item: {\n                id,\n                type,\n                name,\n                image,\n                description,\n                price\n            }\n        })\n        };\n\n    return (\n        <section className=\"list_product\">\n            <div className=\"wishlist_left\">\n                <img src={image}alt={name}/>\n            </div>\n\n            <div className=\"wishlist_right\">\n                <h3>{name}</h3>\n                <p><strong>                    \n                    <CurrencyFormat\n                        value=